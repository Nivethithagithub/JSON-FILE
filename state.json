# create a dictionary of 7 Indian states and their capitals
import json

indian_states = {
    "Maharashtra": "Mumbai",
    "Karnataka": "Bengaluru",
    "Tamil Nadu": "Chennai",
    "Uttar Pradesh": "Lucknow",
    "West Bengal": "Kolkata",
    "Gujarat": "Gandhinagar",
    "Rajasthan": "Jaipur"
}
with open("indian_states.json", "w") as file:
    json.dump(indian_states, file)
with open("indian_states.json", "r") as file:
    contents = json.load(file)
    print(contents)
